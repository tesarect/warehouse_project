
sim-world:
source ~/sim_ws/install/setup.bash
ros2 launch the_construct_office_gazebo warehouse_rb1.launch.xml

cd ~/ros2_ws && colcon build && source install/setup.bash

ros2 launch localization_server localization.launch.py map_file:=warehouse_map_keepout_sim.yaml

source ~/ros2_ws/install/setup.bash && ros2 launch path_planner_server pathplanner.launch.py use_sim_time:=True

python3 ~/ros2_ws/src/warehouse_project/nav2_apps/scripts/move_shelf_to_ship.py

ros2 run attach_shelf approach_service_server

ros2 service call /approach_shelf attach_shelf/srv/GoToLoading "{attach_to_shelf: true}"

